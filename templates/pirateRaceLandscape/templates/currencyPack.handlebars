<div class="content font-face-2" data-categoryid="{{currency_itemId}}">>
  <div class="row">
    <div class="description">
      <label style="{{nameStyle}}">{{name}}</label>
    </div>
  </div>
  <img src="{{imgFilePath}}" class="visual" />
  <div class="row price">
    <label style="{{priceStyle}}}">{{price}}$</label>
    <a class="buy">
      <img src="{{buy}}" />
    </a>
  </div>
</div>
<img src="{{itemSeparator}}" class="separator" />